# Configurações de produção para VisuPlant
NODE_ENV=production
PORT=9000

# Configuração do banco de dados PostgreSQL
DATABASE_URL=postgresql://visuplant_user:visuplant_password@database:5432/visuplant

# Configurações de segurança
# Altere estas variáveis para valores seguros em produção
POSTGRES_DB=visuplant
POSTGRES_USER=visuplant_user
POSTGRES_PASSWORD=visuplant_password

# Configurações do servidor
HOSTNAME=0.0.0.0

# Configurações de timeout (em milissegundos)
APARTAMENTO_TIMEOUT=60000